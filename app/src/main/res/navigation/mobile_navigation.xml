    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.dicodingevent.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/favoriteEventFragment"
        android:name="com.example.dicodingevent.ui.favoriteevent.FavoriteEventFragment"
        android:label="Favorite Event"
        tools:layout="@layout/fragment_favorite_event"/>
    <fragment
